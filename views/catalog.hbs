<head>
      
      <link rel="stylesheet" type="text/css" href="/styles/catalog.css">
          <link rel="stylesheet" type="text/css" href="/styles/menu.css">
           <link rel="stylesheet" type="text/css" href="https://cdn.jsdelivr.net/npm/swiper@11/swiper-bundle.min.css">
</head>
<body>
     {{>menu}}
    {{>sideMenu}}
    <header>
        <div class="container">
            <nav>
                <a href="#" class="logo">TechWorld</a>
                <ul>
                    <li><a >Смартфоны</a></li>
                    <li><a >Ноутбуки</a></li>
                    <li><a >Телевизоры</a></li>
                    <li><a >Аксессуары</a></li>
                    <li><a >О нас</a></li>
                </ul>
                <div class="cart-icon">
                    <a><img src="https://cdn-icons-png.flaticon.com/512/34/34611.png" alt="Корзина"></a>
                </div>
            </nav>
        </div>
    </header>

    <section class="catalog">
        <div class="container">
            <h2>Наш каталог</h2>
            <div class="filter">
                <div class="filter-item">
                    <label for="category">Категория:</label>
                    <select id="category" name="category">
                        <option value="">Все</option>
                        <option value="smartphones">Смартфоны</option>
                        <option value="laptops">Ноутбуки</option>
                        <option value="tvs">Телевизоры</option>
                        <option value="accessories">Аксессуары</option>
                    </select>
                </div>
                <div class="filter-item">
                    <label for="price">Цена:</label>
                    <input type="range" id="price" name="price" min="0" max="100000" step="1000">
                    <span id="price-value">0 ₽</span>
                </div>
            </div>
            <div class="products">
                <div class="product">
                     <div class="swiper">
  
                        <div class="swiper-wrapper">
                            <!-- Slides -->
                            <div class="swiper-slide"><img src="data:image/jpeg;base64," alt="Product 1"></div>
                            <div class="swiper-slide"><img src="https://image.oppo.com/content/dam/oppo/my/mkt/newsroom/stories/advantages-of-smartphone/header.jpg.thumb.webp" alt="Product 2"></div>
                            <div class="swiper-slide"><img src="https://media.wired.com/photos/593284aeaef9a462de98365f/master/w_2560%2Cc_limit/2014-09-23-iphone6-gallery-1.jpg" alt="Product 3"></div>
                            ...
                        </div> 
                            <div class="swiper-button-prev"></div>
                            <div class="swiper-button-next"></div>
                    </div>
                    <h3>Название товара</h3>
                    <p>Цена: 19 990 ₽</p>
                    <a  class="btn">Подробнее</a>
                </div>
                <div class="product">
                    <div class="swiper">
  
                        <div class="swiper-wrapper">
                            <!-- Slides -->
                            <div class="swiper-slide"><img src="data:image/jpeg;base64," alt="Product 1"></div>
                            <div class="swiper-slide"><img src="https://image.oppo.com/content/dam/oppo/my/mkt/newsroom/stories/advantages-of-smartphone/header.jpg.thumb.webp" alt="Product 2"></div>
                            <div class="swiper-slide"><img src="https://media.wired.com/photos/593284aeaef9a462de98365f/master/w_2560%2Cc_limit/2014-09-23-iphone6-gallery-1.jpg" alt="Product 3"></div>
                            ...
                        </div> 
                            <div class="swiper-button-prev"></div>
                            <div class="swiper-button-next"></div>
                    </div>
                    <h3>Название товара</h3>
                    <p>Цена: 29 990 ₽</p>
                    <a  class="btn">Подробнее</a>
                </div>
                </div>
        </div>
    </section>

    <footer>
        <div class="container">
            <p>&copy; 2023 TechWorld. Все права защищены.</p>
        </div>
    </footer>

    <script>
        let nav=document.getElementById("navigation");
let closedNavBTN=document.getElementById("close-icon-nav");
let burger=document.getElementById("burger");




function toogleNav(){
    nav.classList.toggle('open');   
    burger.classList.toggle('open');
   }
   
function closeNav(){
 nav.classList.remove('open');  
 burger.classList.remove('open');  
}

burger.addEventListener('click',toogleNav);
closedNavBTN.addEventListener('click',closeNav);
    </script>
    <script src="https://cdn.jsdelivr.net/npm/swiper@11/swiper-bundle.min.js"></script>
<script>
    const swiper = new Swiper('.swiper', {
  
  direction: 'horizontal',
  loop: false,

  

  
  navigation: {
    nextEl: '.swiper-button-next',
    prevEl: '.swiper-button-prev',
  },

 
});
</script>

</body>